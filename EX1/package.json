{
  "name": "ex1",
  "version": "1.0.0",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon src/server.js",
    "migrate": "npx prisma migrate dev --name init",
    "prestart": "npx prisma generate && npx prisma migrate dev && node prisma/seed.js",
    "start": " node src/server.js",
    "build": "npm run docker:up",
    "docker:up": "docker compose up -d --build",
    "docker:rebuild": "docker compose down && docker compose build --no-cache && docker compose up -d",
    "docker:logs": "docker compose logs -f",
    "docker:stop": "docker compose down",
    "clean": "rmdir /s /q node_modules && del /f /q package-lock.json && npm cache clean --force && npm install"
  },
  "dependencies": {
    "@prisma/client": "^6.17.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "morgan": "^1.10.1",
    "mysql2": "^3.15.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "prisma": "^6.17.0"
  },
  "author": "Kyte Hoang",
  "license": "ISC",
  "description": "A simple Express server project",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ex1.git"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
